.data
	a: .word 0
	aba: .ascii ""
	c: .float 0.0
	e: .double 0.0
	t0: .float 0.0
	cteDouble0: .double 12.12
.text
	li $t0,2
	sw $t0,  a
	la $t0,"a"
	l.s $f0, ($t0)
	s.s $f0,  aba
	lw $t0,9
	mtc1 $t0,$f0
	cvt.s.w $f0,$f0
	s.s $f0, t0
	la $t0,t0
	l.s $f0, ($t0)
	s.s $f0,  c
	la $t0,cteDouble0
	l.s $f0, ($t0)
	s.s $f0,  e
	li $v0, 1
	lw $a0, a
	syscall
	li $v0, 2
	lwc1 $f12, c
	syscall
	li $v0, 3
	ldc1 $f12, e
	syscall
	li $v0, 4
	la $a0, aba
	syscall
	li $v0, 10
	syscall
